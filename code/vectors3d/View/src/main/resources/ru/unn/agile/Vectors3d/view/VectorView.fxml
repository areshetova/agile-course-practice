<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.TitledPane?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>

<TitledPane fx:id="vectorView" collapsible="false" text="" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="ru.unn.agile.Vectors3d.view.VectorView">
  <HBox fillHeight="true" spacing="10.0">
    <children>
      <Label text="X:" />
      <TextField fx:id="xTextField" prefWidth="100.0" HBox.hgrow="ALWAYS" />
      <Label text="Y:" />
      <TextField fx:id="yTextField" prefWidth="100.0" HBox.hgrow="ALWAYS" />
      <Label text="Z:" />
      <TextField fx:id="zTextField" prefWidth="100.0" HBox.hgrow="ALWAYS" />
      <Region prefWidth="50.0" />
      <Button fx:id="normalizeButton" text="Normalize" />
    </children>
  </HBox>
</TitledPane>
